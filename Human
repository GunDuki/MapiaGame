package MapiaGame;

import java.util.Scanner;

public class Human {
	protected static int life;
	protected static String number;
	protected static int votenum;
	public Human(String number){
		this.life=life;
		this.number=number;
	}
	public static int vote(){
		Scanner input = new Scanner(System.in);
		votenum=((int)(Math.random()*(6)));
		return votenum;
	}
}
